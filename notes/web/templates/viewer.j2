<html>
<head>
  <meta charset='utf-8'>
  <link rel="stylesheet" href="/css/katex.min.css">
  <link rel="stylesheet" href="/css/texmath.css">
  <link rel="stylesheet" href="/css/bootstrap.min.css">
  <link rel="stylesheet" href="/css/highlight-js-default.min.css">
  <script src="/js/highlight.min.js"></script>
  <script src="/js/markdown-it.js"></script>
  <script src="/js/katex.min.js"></script>
  <script src="/js/texmath.js"></script>
</head>
<body>

  <div class="row fullwrapper">
    <div class="col-md-2 gutter"></div>
    <div class="col-md-8 pagewrapper">
        <div class="row title-bar">
          <div class="col-md-12">Top Space</div>
        </div>
        <div class="row content">
          <div class="col-md-12 note-content">
            <div id="out"></div>
          </div>
        </div>
        <div class="row footer-bar">
          <div class="col-md-12">Bottom Space</div>
        </div>
    </div>
    <div class="col-md-2 gutter"></div>
  </div>

  <script>
    let md;
    document.addEventListener("DOMContentLoaded", () => {
        const tm = texmath.use(katex);
        md = markdownit().use(tm,{delimiters:'dollars',macros:{"\\RR": "\\mathbb{R}"}});
        out.innerHTML = md.render('{{ file_content }}');
    })
  </script>
  <script>hljs.initHighlightingOnLoad();</script>
</body>
</html>
