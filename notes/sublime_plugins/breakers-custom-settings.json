{
    // http://www.sublimetext.com/docs/3/color_schemes.html
    "variables": {
        // "green": "#FF0000",
        "blue": "hsl(210, 50%, 45%)",
        "blue2": "hsl(180, 36%, 38%)",
        "green": "hsl(114, 31%, 38%)",
        "grey": "hsl(0, 0%, 73%)",
        "grey2": "hsl(0, 0%, 60%)",
        "grey3": "hsl(0, 0%, 20%)",
        "orange": "hsl(32, 93%, 42%)",
        "orange2": "hsl(32, 85%, 55%)",
        "orange3": "hsl(40, 94%, 68%)",
        "pink": "hsl(300, 30%, 48%)",
        "red": "hsl(357, 79%, 55%)",
        "red2": "hsl(13, 93%, 56%)",
        "red3": "hsl(16, 29%, 54%)",
        "white": "hsl(0, 0%, 100%)",
        "white2": "hsl(210, 11%, 85%)",
        "white3": "hsl(204, 10%, 90%)",
        "white4": "hsl(195, 11%, 93%)",
        "white5": "hsl(180, 9%, 96%)",
        "yellow": "hsl(56, 100%, 35%)",
        "yellow2": "hsl(56, 50%, 40%)",
        "brown": "hsl(23, 85%, 37%)",
        "pink2": "hsl(330, 65%, 58%)",
        // "grey2": "hsl(0, 0%, 70%)",
        // "grey3": "hsl(0, 0%, 50%)",
        "green2": "hsl(114, 51%, 43%)",
        "green3": "hsl(180, 53%, 50%)",
        "blue3": "hsl(210, 15%, 24%)",
        "blue4": "hsl(210, 13%, 45%)",
        "blue5": "hsl(180, 36%, 54%)",
        "blue6": "hsl(221, 12%, 69%)",
        "blue7": "hsl(210, 15%, 20%)",
        "heading1": "hsl(0, 79%, 60%)",
        "heading2": "hsl(15, 93%, 56%)",
        "heading3": "hsl(30, 79%, 55%)",
        "heading4": "hsl(45, 79%, 50%)",
        "heading5": "hsl(60, 89%, 45%)",
        "heading6": "hsl(75, 89%, 45%)",
    },
    "globals": {
        // "foreground": "var(green)",
        "background": "var(white5)",
    },
    "rules": [
        {
            // "scope": "string",
            // "foreground": "#00FF00",
        },
        // Set Pure White background on raw blocks for screenshots
        {
            "name": "markup code",
            "scope": "markup.raw",
            "background": "var(white)"
        },
        {
            "name": "LaTeX Start",
            "scope": "markup.latex.start.markdown",
            "foreground": "var(grey3)"
        },
        {
            "name": "LaTeX End",
            "scope": "markup.latex.end.markdown",
            "foreground": "var(grey3)"
        },
        {
            "name": "LaTeX Content",
            "scope": "markup.latex.markdown",
            "foreground": "var(green3)"
        },
        {
            "name": "Definition Term",
            "scope": "noteparser.definition.term",
            "foreground": "var(yellow)"
        },
        {
            "name": "Definition text",
            "scope": "noteparser.definition.definition",
            "foreground": "var(orange)"
        },
        {
            "name": "Metadata Tag",
            "scope": "noteparser.metadata-tag",
            "foreground": "var(red)"
        },
        {
            "name": "Quote Content",
            "scope": "markup.quote",
            "foreground": "var(grey2)"
        },
        {
            "name": "Question Start",
            "scope": "markup.list.question.start",
            "foreground": "var(yellow)"
        },
        {
            "name": "Question Content",
            "scope": "markup.list.question.content",
            "foreground": "var(pink2)"
        },
        {
            "name": "Answer Start",
            "scope": "markup.list.answer.start",
            "foreground": "var(blue5)"
        },
        {
            "name": "Answer Content",
            "scope": "markup.list.answer.content",
            "foreground": "var(green2)"
        },
        {
            "name": "todo items",
            "scope": "markup.list.todo",
            "foreground": "var(yellow2)"
        },
        {
            "name": "markup todo skipped start",
            "scope": "markup.list.todo.skipped.start",
            "foreground": "var(grey2)"
        },
        {
            "name": "markup todo skipped content",
            "scope": "markup.list.todo.skipped.content",
            "foreground": "var(grey2)"
        },
        {
            "name": "markup todo incomplete start",
            "scope": "markup.list.todo.incomplete.start",
            "foreground": "var(red)"
        },
        {
            "name": "todo incomplete content",
            "scope": "markup.list.todo.incomplete.content",
            "foreground": "var(yellow2)"
        },
        {
            "name": "markup todo complete start",
            "scope": "markup.list.todo.complete.start",
            "foreground": "var(blue)"
        },
        {
            "name": "todo complete content",
            "scope": "markup.list.todo.complete.content",
            "foreground": "var(blue4)"
        },
        {
            "name": "strikethrough",
            "scope": "markup.strikethrough",
            "foreground": "var(brown)"
        },
        {
            "name": "strikethrough begin",
            "scope": "punctuation.definition.strikethrough.begin",
            "foreground": "var(brown)"
        },
        {
            "name": "strikethrough end",
            "scope": "punctuation.definition.strikethrough.end",
            "foreground": "var(brown)"
        },
        {
            "name": "markup timestamp",
            "scope": "markup.timestamp",
            "foreground": "var(blue5)"
        },
        {
            "name": "markup h1",
            "scope": "markup.heading.1 punctuation.definition.heading",
            "foreground": "var(heading1)"
        },
        {
            "name": "markup h2",
            "scope": "markup.heading.2 punctuation.definition.heading",
            "foreground": "var(heading2)"
        },
        {
            "name": "markup h3",
            "scope": "markup.heading.3 punctuation.definition.heading",
            "foreground": "var(heading3)"
        },
        {
            "name": "markup h4",
            "scope": "markup.heading.4 punctuation.definition.heading",
            "foreground": "var(heading4)"
        },
        {
            "name": "markup h5",
            "scope": "markup.heading.5 punctuation.definition.heading",
            "foreground": "var(heading5)"
        },
        {
            "name": "markup h6",
            "scope": "markup.heading.6 punctuation.definition.heading",
            "foreground": "var(heading6)"
        },
    ],
}
